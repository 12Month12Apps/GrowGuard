// Debug configuration optimized for performance debugging
// Keeps debug symbols but reduces runtime overhead

// Moderate optimization for debug builds
SWIFT_OPTIMIZATION_LEVEL = -O
GCC_OPTIMIZATION_LEVEL = s

// Keep debug symbols
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
SWIFT_COMPILATION_MODE = wholemodule

// Reduce debug output
GCC_PREPROCESSOR_DEFINITIONS = DEBUG=1

// Core Data optimizations for debug
CORE_DATA_DEBUGGING = NO